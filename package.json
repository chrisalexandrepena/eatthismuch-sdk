{
    "name": "eatthismuch_sdk",
    "version": "0.0.1",
    "type": "module",
    "author": "Chris-Alexandre Pena",
    "files": [
        "/dist"
    ],
    "@parcel/resolver-default": {
        "packageExports": true
    },
    "main": "dist/index.cjs",
    "source": "src/index.ts",
    "types": "dist/index.d.ts",
    "module": "dist/index.mjs",
    "exports": {
        ".": {
            "require": "./dist/index.cjs",
            "import": "./dist/index.mjs",
            "default": "./dist/index.cjs"
        },
        "./index.js": "./dist/index.mjs",
        "./index.mjs": "./dist/index.mjs"
    },
    "engines": {
        "node": ">=16"
    },
    "scripts": {
        "test": "vitest --config vitest.config.ts",
        "build": "rimraf dist && parcel build src/index.ts --config @parcel/config-default",
        "check": "tsc --noEmit",
        "format": "prettier --write src/**/*.ts",
        "lint": "eslint src/**/*.ts",
        "check-prettier": "prettier --check src/**/*.ts"
    },
    "license": "GPL-3.0-only",
    "volta": {
        "node": "18.17.1",
        "yarn": "3.6.0"
    },
    "packageManager": "yarn@3.6.0",
    "devDependencies": {
        "@parcel/config-default": "^2.9.3",
        "@parcel/core": "^2.9.3",
        "@parcel/packager-ts": "2.9.3",
        "@parcel/transformer-typescript-types": "2.9.3",
        "@types/axios": "^0.14.0",
        "@types/node": "^20.4.5",
        "@typescript-eslint/eslint-plugin": "^6.2.0",
        "@typescript-eslint/parser": "^6.2.0",
        "@vitest/coverage-c8": "^0.33.0",
        "eslint": "^8.45.0",
        "eslint-plugin-vitest": "^0.2.8",
        "nock": "^13.3.2",
        "parcel": "^2.9.3",
        "prettier": "^3.0.0",
        "rimraf": "^5.0.1",
        "typescript": "^5.1.6",
        "vitest": "^0.33.0"
    },
    "dependencies": {
        "axios": "^1.4.0",
        "jsforce": "^1.11.1"
    }
}
